<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="" Height="450" Width="750">
    <DockPanel>
        <Button Content="Parse File" x:Name="parse" DockPanel.Dock="Top" FontSize="14" Margin="4"/>
        <ListView x:Name="results" FontFamily="Consolas">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Original" DisplayMemberBinding="{Binding Original}" Width="250"/>
                    <GridViewColumn Header="Parsed" DisplayMemberBinding="{Binding Parsed}" Width="450"/>
                </GridView>
            </ListView.View>
        </ListView>
        <!--<ListBox x:Name="results"/>-->
    </DockPanel>
</Window>